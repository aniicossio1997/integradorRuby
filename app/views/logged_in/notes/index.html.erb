<h2> Notes </h2>
<%= link_to 'nuevo', new_logged_in_note_path,remote: true, class: 'btn btn-primary'%>
<%= render 'flash_message' %>

<table class="table table-hover">
  <thead class="black white-text text-muted">
    <tr class="border-bottom-0">
      <th class="fw-lighter">Title</th>
      <th class="fw-lighter">Date create</th>
      <th class="fw-lighter">Options</th>
    </tr>
  </thead>
  <tbody id="notes_container">
  <%= render @notes %>

  </tbody>
</table>

<!-- Modal -->
<div class="modal fade" id="modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content"> 
    
    </div>
  </div>
</div>