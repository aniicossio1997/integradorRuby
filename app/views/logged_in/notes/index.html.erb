
  <h1 class="d-flex justify-content-center"> <%= t(:notes) %> </h1>
  <hr>
  <%= render 'sub_nav' %>
<table class="table table-hover">
  <thead class="black white-text text-muted">
    <tr class="border-bottom-0">
    <th class="fw-lighter"><%= t(:name) %></th>
    <th class="fw-lighter"><%= t(:date_create)%></th>
    <th class="fw-lighter"><%= t(:date_update)%></th>
    <th class="fw-lighter"><%= t(:options) %></th>
    </tr>
  </thead>
  <tbody id="notes_container">
  <% if !@presenter.notes.empty? %>
  <%= render @presenter.notes %>
<% else %>
<div class="alert alert-warning" role="alert">
 Usted no tiene notas notas asocidas
</div>
<% end %>

  </tbody>
</table>
<br>
<%= will_paginate @presenter.notes%>
