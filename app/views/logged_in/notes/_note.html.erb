<tr id='<%= dom_id(note) %>'>
  <td>
  <div class="row">
  <div class="col-1">
    <i class="large material-icons color-book">description</i>
  </div>
  <div class="col-6">
  <%= note.title %>
  </div>
  
  </td>
  <td><%= date_create(note) %></td>
  <td><%= date_update(note) %></td>

  <td>
  <%= link_to t(:show), logged_in_note_path(note,source:params[:source]), class: 'btn btn-sm btn-outline-info text-right' %>
  <%= link_to t(:export_html), download_logged_in_note_path(note), class: 'btn btn-sm btn-outline-success',id: 'export', target: '_blank'  %>
  <% 
   if  params[:source].nil?
 %>
  <%= link_to t(:edit), edit_logged_in_note_path(note), class: 'btn btn-sm btn-outline-warning' %>
 
  <%= link_to t(:delete), logged_in_note_path(note), method: :delete, data: { confirm: t(:message_confirmation, models: note) }, class: 'btn btn-sm btn-outline-danger' %>

<% end %>
  </td>
</tr>

