<% render 'flash_message'%>

<h1 class="d-flex justify-content-center"> <%= t(:books) %> </h1>
<%= render 'sub_nav' %>
<div class="table-responsive-md">
<table class="table table-hover">
  <thead class="black white-text text-muted">
    <tr class="border-bottom-0">
      <th class="fw-lighter"><%= t(:name) %></th>
      <th class="fw-lighter"><%= t(:date_create)%></th>
      <th class="fw-lighter"><%= t(:options) %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @presenter.books %>
  </tbody>
</table>
</div>
<br>
<div class="d-flex justify-content-center">
<%= will_paginate @presenter.books%>
</div>
<br><br>